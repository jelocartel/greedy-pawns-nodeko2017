{
  "extends": "defaults",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": [1, "only-multiline"],
    "no-console": 0
  },
  "globals": {
    "FB": false,
    "require": true,
    "process": true,
    "__dirname": true,
    "module": true,
    "cervus": true
  }
}
